      svg#preloader.animintro(viewbox='-0 -0 110 110' xmlns='http://www.w3.org/2000/svg' style='z-index: 9999999999999999; width: 50%')
        circle.grande(cx='50%' cy='50%' r='50' style='fill: white; stroke-width: 2px; stroke: black;' vector-effect='non-scaling-stroke')
        circle.mediano(cx='27.5%' cy='50%' r='25' style='fill: none; stroke-width: 2px; stroke: black;' vector-effect='non-scaling-stroke')
        circle.mediano(cx='72.5%' cy='50%' r='25' style='fill: none; stroke-width: 2px; stroke: black;' vector-effect='non-scaling-stroke')
        circle.uno(cx='15.6%' cy='50%' r='12.5' style='fill: none; stroke-width: 2px; stroke: black;' vector-effect='non-scaling-stroke')
        circle.tres(cx='61.5%' cy='50%' r='12.5' style='fill: none; stroke-width: 2px; stroke: black;' vector-effect='non-scaling-stroke')
        circle.dos(cx='38.5%' cy='50%' r='12.5' style='fill: none; stroke-width: 2px; stroke: black;' vector-effect='non-scaling-stroke')
        circle.cuatro(cx='84.4%' cy='50%' r='12.5' style='fill: none; stroke-width: 2px; stroke: black;' vector-effect='non-scaling-stroke')
        text(x='39' y='93' font-size='7' font-weight='bold' fill='black' letter-spacing='12') +0+1234
      h4(style='text-align: center; color:black; font-size: 1.5rem; margin-top: 0; margin-bottom: 0;')
